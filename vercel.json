{
    "version": 2,
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.3",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://consilium-beta.vercel.app"
    }
}